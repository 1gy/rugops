{
  "tasks": {
    "dev": "deno run --allow-read --allow-net --watch ./src/main.ts",
    "build:win": "deno compile --allow-read --allow-net --output rugops --target x86_64-pc-windows-msvc ./src/main.ts",
    "build:linux": "deno compile --allow-read --allow-net --output rugops --target x86_64-unknown-linux-gnu ./src/main.ts"
  }
}
